<Group xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.CourseController">
   <children>
      <Pane fx:id="OriginalPane" prefHeight="768.0" prefWidth="1024.0">
         <children>
            <Button fx:id="addButton" layoutX="13.0" layoutY="14.0" mnemonicParsing="false" onAction="#CreateClick" text="Add Course" />
            <Button fx:id="logoutButton" layoutX="18.0" layoutY="731.0" mnemonicParsing="false" text="Logout" />
            <Button fx:id="courseBox" layoutX="39.0" layoutY="74.0" mnemonicParsing="false" prefHeight="291.0" prefWidth="358.0" style="-fx-background-color: BLUE;" text="Course 1" textFill="WHITE" visible="false">
               <font>
                  <Font size="45.0" />
               </font>
            </Button>
            <TextField fx:id="newName" layoutX="512.0" layoutY="14.0" visible="false" />
            <Label fx:id="renameLabel" layoutX="446.0" layoutY="18.0" prefHeight="17.0" prefWidth="66.0" text="New name:" visible="false" />
            <Button fx:id="renameConfirmB" defaultButton="true" layoutX="671.0" layoutY="14.0" mnemonicParsing="false" onAction="#renameConfirm" text="Confirm" visible="false" />
            <GridPane fx:id="grid1Pane" alignment="CENTER" layoutX="21.0" layoutY="71.0" prefHeight="625.0" prefWidth="979.0" GridPane.columnIndex="0" GridPane.rowIndex="0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="RenameButton" mnemonicParsing="false" onAction="#RenameClick" text="Rename" visible="false" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
                  <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#DeleteClick" snapToPixel="false" text="Delete" visible="false" GridPane.halignment="RIGHT" GridPane.valignment="CENTER">
                     <GridPane.margin>
                        <Insets right="75.0" />
                     </GridPane.margin>
                  </Button>
               </children>
            </GridPane>
         </children>
      </Pane>
   </children>
</Group>
